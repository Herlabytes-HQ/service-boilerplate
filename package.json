{
	"name": "ngx-service-boilerplate",
	"version": "1.0.0",
	"description": "Service boilerplate",
	"main": "src/index.ts",
	"scripts": {
		"test": "jest",
		"build": "tsc",
		"clean": "rm -rf dist",
		"watch": "tsc --watch",
